{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/dist/*", "/dashboard/*", "/config.js", "/config.*.js", "/*.png", "/*.ico", "/*.css", "/*.js"]
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/dist/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/dashboard",
      "rewrite": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/weekly-lineup",
      "rewrite": "/weekly-lineup.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/odds-market",
      "rewrite": "/odds-market.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/*.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/js/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/data/*",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/css/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/config.*.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "geolocation=(), microphone=(), camera=()",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdnjs.cloudflare.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https://a.espncdn.com https://*.espncdn.com https://api.sportsdata.io https://gbsvorchestratorstorage.blob.core.windows.net; connect-src 'self' https://www.greenbiersportventures.com https://site.api.espn.com https://api.sportsdata.io https://api.the-odds-api.com https://*.azurecontainerapps.io https://*.azurewebsites.net https://fonts.googleapis.com https://fonts.gstatic.com https://cdnjs.cloudflare.com https://*.service.signalr.net wss://*.service.signalr.net https://*.signalr.net wss://*.signalr.net https://a.espncdn.com https://*.espncdn.com https://gbsv-signalr.service.signalr.net wss://gbsv-signalr.service.signalr.net; frame-ancestors 'none'"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".svg": "image/svg+xml"
  }
}

