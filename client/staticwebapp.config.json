{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/team-logos/*",
      "/dist/*",
      "/dashboard/*",
      "/config.js",
      "/config.*.js",
      "/*.png",
      "/*.ico",
      "/*.css",
      "/*.js",
      "/manifest.json"
    ]
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/dist/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/dashboard",
      "rewrite": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/weekly-lineup",
      "rewrite": "/weekly-lineup.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/odds-market",
      "rewrite": "/odds-market.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/picks-tracker",
      "rewrite": "/picks-tracker.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/fetch-picks",
      "rewrite": "/fetch-picks.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/*.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/js/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/data/*",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/assets/css/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/config.*.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "geolocation=(), microphone=(), camera=()",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdnjs.cloudflare.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: blob: https://gbsvorchestratorstorage.blob.core.windows.net https://*.blob.core.windows.net https://www.greenbiersportventures.com https://gbsv-endpoint-prod-ekcqbdfya3a8a9dt.z03.azurefd.net https://a.espncdn.com https://*.espncdn.com; connect-src 'self' https://www.greenbiersportventures.com https://gbsv-endpoint-prod-ekcqbdfya3a8a9dt.z03.azurefd.net https://*.azurecontainerapps.io https://site.api.espn.com https://api.sportsdata.io https://api.the-odds-api.com https://fonts.googleapis.com https://fonts.gstatic.com https://cdnjs.cloudflare.com https://*.service.signalr.net wss://*.service.signalr.net https://gbsv-signalr.service.signalr.net wss://gbsv-signalr.service.signalr.net https://gbsvorchestratorstorage.blob.core.windows.net https://*.blob.core.windows.net https://a.espncdn.com https://*.espncdn.com https://gbsv-orchestrator.azurewebsites.net; worker-src 'self'; frame-ancestors 'none'; object-src 'none'; base-uri 'self'; form-action 'self'; upgrade-insecure-requests"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".svg": "image/svg+xml"
  }
}
